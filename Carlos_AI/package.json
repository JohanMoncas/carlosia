{
    "name": "flowise",
    "version": "2.2.7-patch.1",
    "private": true,
    "homepage": "https://flowiseai.com",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "cd packages/server && npm install && npm run build",
        "start": "node packages/server/dist/index.js",
        "dev": "cd packages/server && npm run dev",
        "clean": "rm -rf packages/server/dist && rm -rf node_modules",
        "migration:create": "cd packages/server && npm run migration:create"
    },
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "typescript": "^5.4.5"
    },
    "engines": {
        "node": ">=18.15.0 <19.0.0 || ^20",
        "npm": ">=9"
    },
    "resolutions": {
        "@google/generative-ai": "^0.22.0",
        "@grpc/grpc-js": "^1.10.10",
        "@langchain/core": "0.3.37",
        "openai": "4.82.0"
    },
    "eslintIgnore": [
        "**/dist",
        "**/node_modules"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ]
    }
}
